  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@wordpress/stylelint-config": "^20.0.2",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^27.1.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "^27.1.0",
    "jest-puppeteer": "^5.0.4",
    "postcss": "^8.4.18",
    "stylelint": "^14.9.1",
    "stylelint-config-recess-order": "^3.0.0",
